{
  "name": "@codloy/ui",
  "version": "0.1.0",
  "description": "Codloy UI",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "set NODE_ENV=production && npm run build:typescript && npm run build:tailwind",
    "build:typescript": "tsc",
    "build:tailwind": "tailwindcss -o dist/styles.css --minify",
    "copydist": "cp -r ./dist/* . && rm -rf ./dist",
    "undodist": "git clean -fd",
    "prepublishOnly": "npm run build && npm run copydist",
    "postpublish": "npm run undodist"
  },
  "keywords": [
    "codloy",
    "ui",
    "components"
  ],
  "author": "@codloy",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.0.25",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "tailwind-merge": "^1.8.0"
  }
}
